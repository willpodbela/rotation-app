      </div>
      <div class="inline_block margin_auto bottom50 width190 text_left">
        <img srcset="<%= asset_path 'palace@0,5x.png' %> 1x, <%= asset_path 'palace.png' %> 2x" src="<%= asset_path 'palace@0,5x.png' %>" class="max_width_full" alt="" />
      </div>
      <div class="inline_block margin_auto bottom50 width190 text_center">
        <img srcset="<%= asset_path 'raf@0,5x.png' %> 1x, <%= asset_path 'raf.png' %> 2x" src="<%= asset_path 'raf@0,5x.png' %>" class="max_width_full" alt="" />
      </div>
