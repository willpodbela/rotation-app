<header class="header_11">
  <%= render("shared/nav") %>

  <div style="padding-bottom: 148px;" class="container nopadding">
    <div class="max_width470 inner">
      <h1 class="ubuntu font62 dark_gray top150">Welcome<br /> to a closet without boundaries</h1>
      <div class="font22 light dark_blue top50 text">
        This is The Rotation. A members-only club where the only thing holding back your wardrobe is yourself. Everything you could want, right at your fingertips.
      </div>
      <div class="max_width570 left_block">
        <%= form_tag(sign_up_path, id: "landing-form", remote: true, class: "width470 text_center top80 header_3_form") do %>
          <%= email_field_tag("email", nil, id: "email", class: "width_full radius6", placeholder: "Email Address", required: "required") =%>
          <%= password_field_tag("password", nil, class: "width_full radius6 top30", placeholder: "Password", required: "required") =%>
          <%= button_tag(type: "submit", class: "btn width_full size60 blue radius6 top30") do %>
            Create an Account
          <% end %>
					<div class="top30 medium_gray text">By signing up, you agree to the <a href="#">Terms of Service</a></div>
        <% end %>
      </div>
    </div>
    <img srcset="<%= asset_path 'rotation-app@1x.png' %> 1x, <%= asset_path 'rotation-app@2x.png' %> 2x" src="<%= asset_path 'rotation-app@1x.png' %>" class="left470 bg" alt="" />
  </div>
</header>

<!-- Feature 1 -->

<section class="feature_1 padding_top80 padding_bottom50">
  <div class="container nopadding max_width970">
    <h2 class="font42 ubuntu light dark_gray text_center">How It Works</h2>
    <div class="top70 inner">
      <div class="col-sm-4 text_center bottom50 block">
        <i class="fa fa-shopping-bag font60 light_gray"></i>
        <div class="top20 font28 light dark_gray title">2 New Items
          <br />Every 2 Weeks</div>
        <div class="top30 font18 light dark_blue text">
          Choose from the latest in streetwear style from 100+ top brands.
        </div>
      </div>
      <div class="col-sm-4 text_center bottom50 block">
        <i class="fa fa-box-open font60 light_gray"></i>
        <div class="top20 font28 light dark_gray title">Delivered Straight to
          <br />Your Door</div>
        <div class="top30 font18 light dark_blue text">
          Add a piece into your "rotation" and we'll notify you as soon as its at your door.
        </div>
      </div>
      <div class="col-sm-4 text_center bottom50 block">
        <i class="fa fa-recycle font60 light_gray"></i>
        <div class="top20 font28 light dark_gray title">Free Shipping
          <br />& Dry Cleaning</div>
        <div class="top30 font18 light dark_blue text">
          Send your pieces back dirty using our prepaid label.
        </div>
      </div>
    </div>
  </div>
</section>

<!-- Content 35 -->
