<!-- Pricing Table 7 -->
<hr width="50%">

<section class="pricing_table_7 padding_top70 padding_bottom110">
  <div class="container nopadding dark_blue text_center">
    <h2 class="font42 ubuntu light dark_gray">How much for no limits?</h2>
    <div class="max_width570 margin_auto top15 font18 light">
      Become a member of The Rotation for a low monthly fee.
    </div>
    <div class="max_width570 margin_auto top55 padding_sides100 padding_top55 padding_bottom70 radius10 inner">
      <div class="ubuntu light font28 dark_gray title">Founding Member</div>
      <div class="top25 font14 uppercase semibold spacing20">100 SPOTS</div>
      <div class="top30 light font16 text">
        Be the first to sign up for The Rotation and recieve a special price. New York City residents only.
      </div>
      <a id="price-btn" href="#" class="top45 min_width170 btn size60 transparent_blue border border_blue radius6 font16">Try for $49/mo</a>
    </div>
  </div>
</section>