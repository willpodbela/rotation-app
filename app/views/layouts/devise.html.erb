<% content_for :stylesheets do %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= stylesheet_link_tag    'application', 'https://fonts.googleapis.com/css?family=Hind:300,400,500|Ubuntu:300,400,500', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'application', 'https://use.fontawesome.com/releases/v5.0.10/css/all.css', integrity: 'sha384-+d0P83n9kaQMCwj8F4RJB66tzIwOKmrdb46+porD/OvrJ+37WqIM7UoBtwHO6Nlg', crossorigin: 'anonymous', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'https://maps.googleapis.com/maps/api/js?v=3&key=AIzaSyDP6Ex5S03nvKZJZSvGXsEAi3X_tFkua4U', 'data-turbolinks-track' => true %>
<% end %>

<% content_for :content do %>
  <%= render("shared/hamburger") %>
  <%= render("shared/nav") %>

  <header class="header_11">
    <div style="padding-bottom: 148px;" class="container nopadding">
      <div class="max_width470 inner">
          <div class="max_width570 top120 left_block">
            <%= yield %>
          </div>
        </div>
      <img style="top: 0px;" srcset="<%= asset_path 'rotation-app@1x.png' %> 1x, <%= asset_path 'rotation-app@2x.png' %> 2x" src="<%= asset_path 'rotation-app@1x.png' %>" class="left470 bg" alt="" />
    </div>
  </header>
<% end %>

<% content_for :footer do %>
  <%= render("shared/footer") %>

  <%= render("shared/alerts") %>

  <%= javascript_include_tag 'footer', 'data-turbolinks-track': 'reload' %>
<% end %>
<%= render template: "layouts/application" %>