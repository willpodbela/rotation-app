/* Admin Page CSS */
div.edit td, div.edit th,
div.new td, div.new th, 
.admin_table {
  width: max-content;
}
.admin_table td, .admin_table th {
  padding: 0px 5px;
}
.admin_table select {
  min-width: 150px;
  width: auto;
}
.admin_table .black-border-right {
  border-right: 1px solid #000;
}
.admin_table .actions {
  border-left: 1px solid #000;
}
div.edit td input, div.edit td textarea,
div.new td input, div.new td textarea {
  padding: 5px;
  border: 1px solid black;
  border-radius: 4px;
}
div.edit input[type=submit], 
div.new input[type=submit] {
    background-color: #4CAF50;
    color: white;
    padding: 5px 10px;
    margin: 8px;
    border-radius: 4px;
    cursor: pointer;
}
div.edit input[type=submit]:hover,
div.new input[type=submit]:hover {
    background-color: #45a049;
}
div.items img {
  display: block;
  max-width:200px;
  max-height:200px;
  width: auto;
  height: auto;
}
td.slim-text {
  max-width: 300px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* Blocks layout */
body {
  font-family: 'Hind', sans-serif;
  font-weight: 400;
  font-size: 16px;
  line-height: 1.5;
  -webkit-font-smoothing: antialiased;
  /* 	background:#ecf0f1; */
}

header, section {
  overflow: hidden;
}

header {
  position: relative;
}

header nav {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1000;
}

nav {
  width: 100%;
}

nav.transparent {
  background: transparent;
}

a {
  color: #2c3e50;
  cursor: pointer;
  -webkit-transition: all 0.2s;
  -o-transition: all 0.2s;
  transition: all 0.2s;
}

a:hover {
  color: #4a6580;
  text-decoration: none;
}

a:focus {
  color: #4a6580;
  text-decoration: none;
  outline: none;
}

button {
  border: none;
  outline: none !important;
  -webkit-transition: all 0.2s;
  -o-transition: all 0.2s;
  transition: all 0.2s;
}

.btn.focus, .btn:focus {
  -webkit-box-shadow: none;
  box-shadow: none;
}

input, textarea {
  border: 0;
  outline: none;
  background: transparent;
  color: #2c3e50;
  resize: none;
}

label {
  font-weight: 400;
  cursor: pointer;
}

