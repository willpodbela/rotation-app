.alert {
  display: none;
  position: fixed;
  top: 50px;
  left: 50%;
  -webkit-transform: translateX(-50%);
  -ms-transform: translateX(-50%);
  transform: translateX(-50%);
  width: 100%;
  max-width: 970px;
  z-index: 2000;
}

.alert-dismissable .close, .alert-dismissible .close {
  top: 0;
}

.container {
  position: relative;
}

/* Navigation Mobile */
.navigation_mobile {
  position: fixed;
  top: 0px;
  left: 0px;
  height: 100%;
  width: 0px;
  overflow-x: hidden;
  overflow-y: auto;
  z-index: 1000;
  background: #000000;
  -webkit-transition: width 0.2s;
  -o-transition: width 0.2s;
  transition: width 0.2s;
}

.navigation_mobile.opened {
  width: 320px;
}

.navigation_mobile.opened .close_menu {
  display: block;
}

.open_menu, .close_menu {
  display: none;
  top: 20px;
  width: 40px;
  height: 40px;
  color: #2c3e50;
  font-size: 20px;
  line-height: 40px;
  text-align: center;
  z-index: 990;
}

.open_menu.visible, .close_menu.visible {
  display: block;
}

.open_menu.white, .close_menu.white {
  color: #ffffff;
}

.open_menu.blue, .close_menu.blue {
  color: #2b48e7;
}

.open_menu.right, .close_menu.right {
  left: auto;
  right: 20px;
}

.open_menu {
  position: fixed;
  left: 20px;
}

.close_menu {
  right: 20px;
  position: absolute;
}
